<h2><%= @search.count%> results</h2>

<% @search.each do |station|  %>
  <div class="station">
    <span class="name"><%=station[:station_name]%></span>
    <span class="address"><%=station[:street_address]%></span>
    <span class="fuel_type"><%=station[:fuel_type_code]%></span>
    <span class="access_times"><%=station[:access_day_time]%></span>
  </div>
<% end %>